<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<title>ASIGNAR</title>
</head>
<script src = "https://code.jquery.com/jquery-1.10.2.js"></script>
<body>

	<h3>Asignar pasajero</h3>

	<form action="/procesar" method="post">

		<input type="text" name="ID_PASAJERO" id="I_P" placeholder="ID PASAJERO">

		<br><br>

		<input type="text" name="ID_AERONAVE" id="I_A" placeholder="ID AERONAVE">

		<br><br>

		<input type="submit">

	</form>

</body>
<script>
$('#submit')
.click(function(e){
e.preventDefault();
id_pasajero = document.getElementById('I_P').value;
id_aeronave = document.getElementById('I_A').value;
var alien = '{"Id_Pasajero":'+id_pasajero+', "Id_Aero": '+id_aeronave+'}';
json=JSON.parse(alien)
$.ajax({
	data: json,
	url: '/Viaje',
	type: 'post',

	});
});
</script>
</html>
